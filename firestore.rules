rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // Disallow client-side creation, allow only server-side (Admin SDK)
    match /landscapeEstimates/{estimateId} {
      allow create: if false; // Changed from true to false
      allow read, update, delete: if false;
    }

    // Deny all access to any other collections/documents by default
    match /{document=**} {
      allow read, write: if false;
    }
  }
}