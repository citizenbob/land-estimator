rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /landscapeEstimates/{estimateId} {
      allow create: if true;
      allow read: if true;
      allow update, delete: if false;
    }
    match /{document=**} {
      allow read, write: if false; // Everything else locked down
    }
  }
}