{
  "name": "land-estimator",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "bash -c 'echo \"Verifying CDN data availability...\"; curl -sf https://storage.googleapis.com/land-estimator-29ee9.firebasestorage.app/cdn/version-manifest.json > /dev/null && echo \"✅ CDN version manifest accessible\" || echo \"⚠️ Warning: CDN version manifest not accessible\"'",
    "build:full": "NODE_OPTIONS='--max-old-space-size=16384' yarn build",
    "build:data": "cd src/config/scripts && python3 ingest_scripts.py --dataset-size=large",
    "build:tokens": "npx style-dictionary build --config ./style-dictionary.config.mjs",
    "backup:create": "tsx src/config/scripts/create_emergency_backups.ts",
    "start": "next start",
    "clean": "yarn format && yarn lint:fix",
    "hooks:setup": "bash setup-hooks.sh",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,css,scss}' '!src/data/*.json'",
    "check:format": "prettier --check 'src/**/*.{ts,tsx,css,scss}' '!src/data/*.json'",
    "check:types": "tsc --noEmit",
    "hooks:run": "yarn lint && yarn check:format && yarn check:types",
    "test": "vitest --watch",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:python": "pip install -r src/config/scripts/requirements.txt && python -m unittest discover src/data 'test_*.py'",
    "test:e2e": "bash -c 'echo \"Starting fresh dev server...\"; pkill -f \"next dev\" || true; sleep 2; yarn dev & DEV_PID=$!; sleep 10; echo \"Waiting for server to be ready...\"; until curl -s http://localhost:3000 > /dev/null; do sleep 1; done; echo \"Running E2E tests...\"; cypress run; TEST_RESULT=$?; echo \"Stopping dev server...\"; kill $DEV_PID || true; sleep 2; exit $TEST_RESULT'",
    "test:e2e:open": "cypress open",
    "test:all": "yarn test:run && yarn test:python && yarn test:e2e",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@vercel/blob": "^1.1.1",
    "dotenv": "^16.5.0",
    "fflate": "^0.8.2",
    "firebase": "^11.5.0",
    "firebase-admin": "^13.3.0",
    "flexsearch": "^0.8.205",
    "framer-motion": "^12.7.4",
    "mixpanel-browser": "^2.62.0",
    "next": "^15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^6.1.16"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.3.1",
    "@storybook/addon-a11y": "^8.6.4",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-onboarding": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/components": "^8.6.11",
    "@storybook/core-events": "^8.6.11",
    "@storybook/experimental-addon-test": "^8.6.6",
    "@storybook/experimental-nextjs-vite": "8.6.11",
    "@storybook/jest": "^0.2.3",
    "@storybook/manager": "^8.6.11",
    "@storybook/manager-api": "^8.6.11",
    "@storybook/test": "^8.6.11",
    "@storybook/test-runner": "^0.22.0",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^8.6.11",
    "@tailwindcss/postcss": "^4.0.14",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/cypress": "^1.1.6",
    "@types/mixpanel-browser": "^2.54.0",
    "@types/node": "^20",
    "@types/react": "19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitest/browser": "^3.0.8",
    "@vitest/coverage-istanbul": "^3.0.8",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "cypress": "^14.2.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.4",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.12.0",
    "jsdom": "^26.0.0",
    "playwright": "^1.51.0",
    "postcss": "^8.1.0",
    "prettier": "^3.2.4",
    "storybook": "^8.6.4",
    "storybook_vitest_addon": "0.1.8",
    "style-dictionary": "^4.3.3",
    "tailwindcss": "^4.0.13",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vite": "^6.0.0",
    "vitest": "^3.0.8",
    "vitest-axe": "^0.1.0",
    "wrangler": "^4.22.0",
    "yarn-deduplicate": "^6.0.2"
  },
  "resolutions": {
    "@storybook/components": "8.1.5",
    "@storybook/manager-api": "8.1.5",
    "@storybook/theming": "8.1.5",
    "@storybook/core-events": "8.1.5",
    "@zip.js/zip.js": "2.7.59",
    "vitest": "3.0.8",
    "@vitest/coverage-istanbul": "3.0.8"
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=18.0.0"
  }
}
